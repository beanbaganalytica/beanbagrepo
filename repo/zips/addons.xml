<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="plugin.program.kodibuildwizard" name="[COLOR cyan]kodibuild[/COLOR]Wizard" version="1.0.7" provider-name="ndeering5">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
        <import addon="script.kodi.android.update" optional="true" version="1.1.7" />
        <import addon="script.speedtester" optional="true" version="1.1.2" />
    </requires>
    
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>
    
    <extension point="xbmc.service" library="startup.py" start="startup" />
    
    <extension point="xbmc.addon.metadata">
        <summary lang="en">[COLOR cyan]kodibuild[/COLOR]Wizard</summary>
        <description lang="en">[COLOR cyan]kodibuild[/COLOR]Wizard offers many maintenance features, selective add-on data saving, and automated build installs.</description>
        <platform>all</platform>
        <reuselanguageinvoker>false</reuselanguageinvoker>
        <source>https://github.com/ndeering5/plugin.program.kodibuildwizard</source>
        <news>
1.0.0
 - Initial Commit
        </news>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.png</fanart>
        </assets>
    </extension>
